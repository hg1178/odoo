<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
        <!--define access group category-->
        <record id="student_managment_access_categ" model="ir.module.category">
            <field name="name">Student Managment</field>
        </record>

        <!--group of sttaf-->
        <record id="sttaf_access_group" model="res.groups">
            <field name="name">Sttaf</field>
            <field name="help">Student office staff</field>
            <field name="category_id" ref="student_managment_access_categ"/>
        </record>

        <!--group of manager-->
        <record id="manager_access_group" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="student_managment_access_categ"/>
            <field name="help">Office of manager</field>
            <field name="implied_ids" eval="[(4, ref('sttaf_access_group'))]"/>
        </record>

    </data>
</openerp>