<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
        <template id="student_profile_template">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="report.external_layout">
                        <div class="page">
                            <group string="General Information">
                                <br></br>
                                <h4>General Student Information</h4>
                                <table class="table-bordered" width="188">
                                    <tr>
                                        <td>
                                            <b>Ministry University No.:</b>
                                        </td>
                                        <td width="94">
                                            <b><span t-field="o.uni_no"/></b>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td width="94">
                                            <b>Universty Seat No.:</b>
                                        </td>
                                        <td>
                                            <b><span t-field="o.seat_no"/></b>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <b>Name.:</b>
                                        </td>
                                        <td width="94">
                                            <span t-field="o.name"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <b>Date Of Birth.:</b>
                                        </td>
                                        <td width="94">
                                            <span t-field="o.dob"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <b>Age.:</b>
                                        </td>
                                        <td width="94">
                                            <span t-field="o.age"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <b>Email.:</b>
                                        </td>
                                        <td width="94">
                                            <span t-field="o.email"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <b>Phone.:</b>
                                        </td>
                                        <td width="94">
                                            <span t-field="o.phone"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <b>Degree.:</b>
                                        </td>
                                        <td width="94">
                                            <span t-field="o.degree_id"/>
                                        </td>
                                    </tr>
                                </table>
                                <hr></hr>
                            </group>
                            <group string="School Result Details">
                                <h4>School Result Information</h4>
                                <table class="table-bordered" width="188">
                                    <thead>
                                        <tr>
                                            <th>Subject</th>
                                            <th>Result</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <t t-foreach="o.result_ids" t-as="n">
                                            <tr>
                                                <td><span t-field="n.subject_id"/></td>
                                                <td><span t-field="n.results"/></td>
                                            </tr>
                                        </t>
                                    </tbody>
                                </table>
                                <hr></hr>
                            </group>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</openerp>